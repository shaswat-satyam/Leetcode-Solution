class Solution {
    public int maxArea(int[] height) {
        int low = 0;
        int high = height.length-1;
        int result = 0;
        while(low <= high){
            if(height[low] < height[high]){
                result = Math.max(result,height[low] * (high - low));
                low += 1;
            } else {
                result = Math.max(result, height[high] * (high - low));
                high -= 1;
            }
        }
        return result;
    }
}